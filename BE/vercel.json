{
    "version": 2,
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "api/$1.js"
      },
      {
        "src": "/images/(.*)",
        "headers": { "Cache-Control": "public, max-age=31536000" },
        "dest": "/images/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/$1.html"
      }
    ],
    "env": {
      "API_KEY": "my-api-key",
      "NODE_ENV": "production"
    }
  }
  